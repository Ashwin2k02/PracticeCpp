#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;
float dist(float x1,float x2,float y1,float y2)
{
    float t,u,n;
    float d;
    t=(y1-x1)*(y1-x1);//9
    u=(y2-x2)*(y2-x2);//1
    d=sqrt(t+u);//3.16
    return d;
}
int main()
{
   float a,b,c,d,e;
    cin>>a>>b>>c>>d;
    e=dist(a,b,c,d);
    cout<<fixed<<setprecision(2)<<"Distance between the 2 points is "<<e;
}
