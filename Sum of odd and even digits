#include<iostream>
using namespace std;
int main()
{
    int num,num1,num2=0,even=0,odd=0;
    cin>>num;
    while(num>0)
    {
        num2=num%10; 
        num=num/10;
        if(num2%2==0)
        {
            even=even+num2;
        }
        else
        {
            odd=odd+num2;
        }
    }
    cout<<even<<" ";
    cout<<odd;
}
